{
  "name": "electron-node-hid-jest-ts-problem",
  "version": "0.0.0",
  "description": "Demonstrates a problem in combining node-hid rebuilt for electron and jest.",
  "private": true,
  "main": "Program.js",
  "scripts": {
    "start": "electron .",
    "test": "jest",
    "__postinstall": "electron-rebuild --force"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/jest": "^21.1.10",
    "@types/node": "^8.0.58",
    "@types/node-hid": "^0.5.2",
    "@types/react": "^16.0.29",
    "@types/react-dom": "^16.0.3",
    "electron": "~1.7.8",
    "electron-rebuild": "^1.6.0",
    "jest": "^22.0.4",
    "ts-jest": "^22.0.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "node-hid": "^0.5.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
